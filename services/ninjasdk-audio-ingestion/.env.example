# NinjaSDK Audio Ingestion Service Configuration

# NinjaSDK Settings
NINJASDK_HOST=wss://wss.vdo.ninja
NINJASDK_ROOM=ooblex-audio
NINJASDK_PASSWORD=
DEBUG=false

# Redis Configuration
REDIS_URL=redis://localhost:6379

# RabbitMQ Configuration
RABBITMQ_URL=amqp://guest:guest@localhost:5672

# Audio Processing Configuration
AUDIO_SAMPLE_RATE=16000
AUDIO_CHANNELS=1
AUDIO_CHUNK_DURATION_MS=1000
AUDIO_FORMAT=pcm16

# Logging
LOG_LEVEL=info
